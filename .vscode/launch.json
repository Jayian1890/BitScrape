{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "CLI Debug",
        "type": "lldb",
        "request": "launch",
        "program": "${workspaceFolder}/build/bin/bitscrape_cli",
        "args": [],
        "cwd": "${workspaceFolder}",
        "preLaunchTask": "Build CLI App (Debug)",
        "stopOnEntry": false,
        "console": "integratedTerminal",
      },
      {
        "name": "Attach to Process by PID",
        "type": "lldb",
        "request": "attach",
        "pid": "${command:pickProcess}",
      },
    ],
  }
  